{
  "name": "developers.chrome.com",
  "version": "1.0.0",
  "description": "The source for developers.chrome.com",
  "private": true,
  "engines": {
    "node": ">=12.18.3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all clean gulp rollup --parallel --race gulp:watch rollup:watch eleventy:watch server",
    "eleventy:watch": "npx eleventy --watch",
    "eleventy": "npx eleventy",
    "fix": "gts fix",
    "gulp:watch": "npx gulp watch",
    "gulp": "npx gulp",
    "lint:js": "gts check",
    "lint:md": "remark .",
    "lint:scss": "stylelint **/*.scss",
    "lint:types": "tsc --noEmit",
    "lint": "npm-run-all lint:js lint:scss lint:md lint:types",
    "production": "NODE_ENV=production npm-run-all clean gulp rollup eleventy",
    "snapshots": "percy snapshot dist",
    "percy": "npm-run-all production snapshots",
    "rollup:watch": "npx chokidar \"site/**/*.js\" -c \"npx rollup -c\"",
    "rollup": "npx rollup -c",
    "server": "node ./server",
    "stage": "NODE_ENV=production npm run production && gcloud app deploy --project=chrome-apps-doc-staging-2020",
    "start": "node ./server",
    "test:watch": "ava --watch",
    "test": "nyc ava"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-cache-assets": "^2.0.3",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "chokidar-cli": "^2.1.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "gorko": "^0.2.1",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-sass": "^4.1.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.22.2",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@types/node": "^13.11.1",
    "ava": "^3.12.1",
    "csso": "^4.0.3",
    "eslint-plugin-ava": "^11.0.0",
    "gts": "^2.0.2",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "percy": "^0.28.2",
    "purgecss": "^2.3.0",
    "remark-cli": "^8.0.1",
    "remark-frontmatter": "^2.0.0",
    "remark-lint": "^7.0.1",
    "remark-lint-are-links-valid": "^1.0.3",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-lint-no-duplicate-headings-in-section": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^3.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "remark-validate-links": "^10.0.2",
    "require-yml": "^2.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "^3.8.3",
    "unified-lint-rule": "^1.0.6",
    "unist-util-map": "^2.0.1",
    "unist-util-to-list-of-char": "^0.1.2",
    "unist-util-visit": "^2.0.3"
  }
}
